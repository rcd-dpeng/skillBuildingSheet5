<AdminScreen>:
    name: 'admin'
    FloatLayout:
        size_hint: None, None

        DPEAButton:
            background_color: 0, 0, 0, 0
            background_normal: ''
            size_hint: None, None
            text: 'Back to Game'
            id: back
            center_x: root.center_x
            center_y: root.center_y + 260
            color: 0.019, 0.337, 1, 1
            size: 280, 80
            on_press: root.backButtonDown()
            on_release: root.manager.current = 'examples'
            on_touch_up: root.resetColors()

        Label:
            text: ' '
            id: pw
            font_size: 60
            color: 1, 1, 1, 1
            bold: True
            outline_width: self.font_size * 0.1
            outline_color: 0, 0, 0
            center_x: root.center_x
            center_y: root.center_y + 160

        DPEAButton:
            background_color: 0, 0, 0, 0
            background_normal: ''
            size_hint: None, None
            text: '1'
            id: one
            center_x: root.center_x - 100
            center_y: root.center_y + 60
            color: 0.019, 0.337, 1, 1
            size: 80, 80
            on_press: root.oneButtonDown()
            on_release: root.addNum(1)
            on_touch_up: root.resetColors()

        DPEAButton:
